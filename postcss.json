{
    "input": "src/assets/css/*.css",
    "output": "build/assets/css/main.css",
    "use": [
        "postcss-import",
        "postcss-at-rules-variables",
        "postcss-custom-properties",
        "postcss-for",
        "postcss-each",
        "postcss-calc",
        "postcss-custom-media",
        "postcss-nesting",
        "autoprefixer",
        "postcss-csso"
    ],
    "autoprefixer": {
        "browsers": "last 2 versions"
    }
}
